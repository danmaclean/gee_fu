<%- presenter = FeaturePresenter.new(feature) -%>
<tr>
  <td><%= link_to feature.id, edit_feature_path(feature) %></td>
  <td><%= feature.seqid %></td>
  <td><%= feature.feature %></td>
  <td><%= feature.start %></td>
  <td><%= feature.end %></td>
  <td><%= feature.strand %></td>
  <td><%= presenter.attributes %></td>
  <td><%=l feature.created_at.to_date, format: :gee_fu %></td>
  <td><%= presenter.parent_list %></td>
</tr>